<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Training.Views.ToJSONPage">
    <ContentPage.Content>
        <StackLayout Padding="5">
            <Label Text="ToJSON String" FontSize="Large" HorizontalOptions="StartAndExpand" />
            <Editor Text="{Binding JSONString, Mode=OneWay}" AutoSize="TextChanges" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>