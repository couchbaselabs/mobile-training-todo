<?xml version='1.0' encoding='utf-8'?>
<Context>
    <Environment name="databaseDirectory"
                 value="/tmp/com.couchbase.lite.todo/database"
                 type="java.lang.String"/>
    <Environment name="syncGatewayUrl"
                 value="ws://localhost:4984/todo"
                 type="java.lang.String"/>
    <Environment name="loggingEnabled"
                 value="true"
                 type="java.lang.Boolean"/>
    <Environment name="loginRequired"
                 value="true"
                 type="java.lang.Boolean"/>
    <!-- Value for conflict resolution can be local/remote -->
    <Environment name="customConflictResolution"
                 value="default"
                 type="java.lang.String"/>
    <Environment name="maxRetries"
                 value="0"
                 type="java.lang.Integer"/>
    <Environment name="waitTime"
                 value="0"
                 type="java.lang.Integer"/>
</Context>